package product;

public class Order extends Product {

	public Order() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Order(String name, int price) {
		super(name, price);
		// TODO Auto-generated constructor stub
	}

	private int count;
	private int total;
	
	public int getCount() {
		return count;
	}
	public void setCount(int count) {
		this.count = count;
	}
	public int getTotal() {
		return total;
	}
	public void setTotal(int total) {
		this.total = total;
	}
	
	public void print() {
		System.out.println(super.getName()+":"+super.getPrice());
		System.out.println(count+"개 주문 => 금액:");
	}
	
	
}
